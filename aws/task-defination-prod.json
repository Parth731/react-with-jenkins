{
  "requiresCompatibilities": ["FARGATE"],
  "family": "LearnJenkinsApp-New-TaskDefinition-Prod",
  "containerDefinitions": [
    {
      "name": "learnjenkinsapp",
      "image": "597081897916.dkr.ecr.ap-south-1.amazonaws.com/learnjenkinsapp:1.0.47",
      "portMappings": [
        {
          "name": "ngnix-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true
    }
  ],
  "volumes": [],
  "networkMode": "awsvpc",
  "memory": "512",
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::597081897916:role/ecsTaskExecutionRole"
}
